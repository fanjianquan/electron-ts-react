{
  "name": "electron-ts-react",
  "version": "0.0.1",
  "description": "electron typescript react boilerplate",
  "main": "./dist/main.js",
  "scripts": {
    "build-main": "webpack --mode production --config webpack.main.config.js",
    "build-renderer": "webpack --config webpack.renderer.prd.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config webpack.main.config.js && electron ./dist/main.js",
    "start-dev": "npm run start-renderer-dev",
    "prestart": "npm run build",
    "start": "electron .",
    "lint": "tslint '{src,test,mocks}/**/*.{ts,tsx}' --project ./tsconfig.json",
    "precommit": "lint-staged",
    "test": "jest '(\\/test\\/(?!e2e/)).*'",
    "pretest:e2e": "npm run build",
    "test:e2e": "jest '(\\/test\\/e2e/).*'",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "lint-staged": {
    "{src,test,mocks}/**/*.{js,ts,tsx,json,css,scss,md}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ],
    "{src,test,mocks}/**/*.{js,ts,tsx}": [
      "tslint --project ./tsconfig.json --fix",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.+\\.spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(s?css|sass)$": "<rootDir>/mocks/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:fanjianquan/electron-ts-react.git"
  },
  "author": {
    "name": "Jianquan Fan",
    "email": "jianquan.fan@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fanjianquan/electron-ts-react/issues"
  },
  "homepage": "https://github.com/fanjianquan/electron-ts-react",
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^7.0.5",
    "@types/react-test-renderer": "^16.8.1",
    "@types/webdriverio": "^4.8.0",
    "@types/webpack-env": "^1.13.9",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^2.1.1",
    "electron": "^7.2.4",
    "electron-builder": "^20.39.0",
    "electron-devtools-installer": "^2.2.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "react-hot-loader": "^4.8.0",
    "react-test-renderer": "^16.8.5",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.4000",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1"
  }
}
